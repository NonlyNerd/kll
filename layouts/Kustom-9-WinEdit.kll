# Bottom Row
U"Z" : U"Ctrl" + U"Z";
U"X" : U"Ctrl" + U"X";
U"C" : U"Ctrl" + U"C";
U"V" : U"Ctrl" + U"V";
U"B" : U"Left";
U"`" : U"Ctrl" + U"`";

U"=" : U"Ctrl" + U"=";
